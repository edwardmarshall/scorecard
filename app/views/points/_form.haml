= form_for(@point) do |f|
  - if @point.errors.any?
    #error_explanation
      %h2
        = pluralize(@point.errors.count, "error")
        prohibited this point from being saved:
      %ul
        - @point.errors.full_messages.each do |msg|
          %li = msg

  .field
    = f.label :antler_id
    = f.number_field :antler_id
  
  .field
    = f.label :lg_point
    = f.number_field :lg_point

  .actions
    = f.submit
